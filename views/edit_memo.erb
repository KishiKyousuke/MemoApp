<body>
  <%@selected_memo.each do |memo|%>
    <form action="/<%=@id%>" method="post">
      <textarea name="text" cols="30" rows="10"><%=memo["title"]%><%=memo["content"]%></textarea>
      <input type="hidden" name="_method" value="patch">
      <input type="submit" value="変更">
    </form>
  <%end%>
</body>
